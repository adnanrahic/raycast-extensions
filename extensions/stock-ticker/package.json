{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stock-ticker",
  "title": "Stock Ticker",
  "description": "Track your favorite stock tickers.",
  "icon": "icon.png",
  "author": "adnanrahic",
  "categories": [
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Stock Tickers",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.32.1",
    "canvas2svg": "^1.0.16",
    "chart.js": "^3.7.1",
    "dedent-js": "^1.0.1",
    "htm": "^3.1.0",
    "moment": "^2.29.3",
    "nanoid": "^3.3.1",
    "node-fetch": "^3.0.0",
    "react-chartjs-2": "^4.1.0",
    "svg-line-chart": "^0.4.0",
    "vhtml": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/vhtml": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
